<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.sb2.training.model.Doctor" />
	<class name="com.sb2.training.model.impl.DoctorImpl" table="mysbns_doctor">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="docId" type="long">
			<generator class="assigned" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="speciality" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>